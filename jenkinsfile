pipeline {
    agent any
    stages{
        stage('configration Ec2 and give the test page'){
            steps{
                    sh '''
                            ansible-playbook -i /home/ubuntu/Devops-Project/inventory \
                                                  /home/ubuntu/Devops-Project/playbook.yml \
                                                     --private-key Devops-Project/test.pem \
                                                        -u ubuntu -v
                                                                                '''
                  }  }

        stage('Build'){
            steps{
                    echo ">>>>>  stage 'Build' is commplete.........."
                  }
                        }
         stage('Test'){
            steps{
                    echo ">>>>>  stage 'Test' is commplete..........."                
                  }
                       }   
         stage('Deploy'){
            steps{
                    echo ">>>>>  stage 'Deploy' is commplete........."                 
                  }
                       }                                   
        }

    }
